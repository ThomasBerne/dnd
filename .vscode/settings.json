{
  "editor.rulers": [80],
  "editor.tabSize": 2,

  "i18n-ally.displayLanguage": "fr",
  "i18n-ally.keystyle": "nested",

  "i18n-ally.localesPaths": ["**/*/i18n/locales"],
  "prettier.configPath": ".prettierrc.json",

  "typescript.tsdk": "node_modules/typescript/lib",
  
  "vue.editor.reactivityVisualization": false,
  "vue.editor.templateInterpolationDecorators": false,
  "vue.suggest.componentNameCasing": "alwaysPascalCase",
  "vue.suggest.propNameCasing": "alwaysCamelCase",
  "yaml.schemas": {
    "https://raw.githubusercontent.com/DavidAnson/markdownlint/main/schema/markdownlint-config-schema.json": "untitled:Untitled-1"
  }
}
